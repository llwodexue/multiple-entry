{
  "name": "h3bpm-front",
  "version": "1.0.0",
  "scripts": {
    "dev": "vue-cli-service serve",
    "dev:admin": "cross-env DEV_MODULE=admin vue-cli-service serve",
    "dev:mobile": "cross-env DEV_MODULE=mobile vue-cli-service serve",
    "build": "cross-env NODE_ENV=production node build/index.js",
    "build:admin": "cross-env NODE_ENV=production node build/index.js admin",
    "build:mobile": "cross-env NODE_ENV=production node build/index.js mobile",
    "format": "prettier --write \"./**/*.{html,vue,js,jsx}\"",
    "lint": "eslint --ext .js,.vue,.jsx src",
    "clean": "rimraf node_modules",
    "inspectD": "vue-cli-service inspect --mode development >> webpack.config.dev.js",
    "inspectP": "vue-cli-service inspect --mode production >> webpack.config.prod.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "core-js": "^3.26.0",
    "crypto-js": "^3.3.0",
    "element-ui": "^2.15.13",
    "js-cookie": "2.2.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "vant": "^2.12.53",
    "vue": "^2.7.14",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "autoprefixer": "^9.5.1",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "chalk": "4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "rimraf": "3.0.2",
    "path-browserify": "^1.0.1",
    "sass": "^1.65.1",
    "sass-loader": "^12.6.0",
    "svg-sprite-loader": "^6.0.11",
    "tasksfile": "^5.1.1"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  }
}
